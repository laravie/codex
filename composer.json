{
    "name": "laravie/codex",
    "description": "PHP library for building API Client SDK for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravie\\Codex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravie\\Codex\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0.0 <8.3.0",
        "laravie/codex-common": "^2.1"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "laravie/codex-filter": "^2.0",
        "mockery/mockery": "^1.4.4",
        "php-http/guzzle7-adapter": "^1.0",
        "php-http/multipart-stream-builder": "^1.0",
        "phpstan/phpstan": "^1.10.5",
        "phpunit/phpunit": "^9.6"
    },
    "suggest": {
        "laravie/codex-filter": "Content filtering for Laravie Codex (^2.0)",
        "php-http/multipart-stream-builder": "Allow to send multipart form request (^1.0)",
        "phpunit/phpunit": "Allow to use Codex Faker request during testing (^9.6)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev"
}
