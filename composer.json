{
    "name": "laravie/codex",
    "description": "PHP library for building API Client SDK for PHP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravie\\Codex\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravie\\Codex\\TestCase\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "guzzlehttp/psr7": "^1.4",
        "igorw/get-in": "^1.0",
        "php-http/client-common": "^1.6",
        "php-http/client-implementation": "^1.0",
        "php-http/discovery": "^1.3",
        "php-http/httplug": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "php-http/curl-client": "^1.7",
        "phpunit/phpunit": "~7.0"
    },
    "suggest": {
        "php-http/multipart-stream-builder": "Allow to send multipart form request (^1.0)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
